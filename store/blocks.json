{
  "header": {
    "blocks": ["telemarketing", "logo"]
  },
  "header.full": {
    "blocks": ["header-layout.desktop", "header-layout.mobile"]
  },
  "header-layout.desktop": {
    "children": ["header-row#desktop-1", "header-row#desktop-2"]
  },
  "header-row#desktop-1": {
    "children": ["telemarketing"],
    "props": {
      "fullWidth": true
    }
  },
  "header-row#desktop-2": {
    "children": [
      "logo#header",
      "header-spacer",
      "search-bar",
      "login",
      "minicart"
    ],
    "props": {
      "sticky": true
    }
  },
  "header-row#desktop-3": {
    "children": ["category-menu"]
  },
  "header-layout.mobile": {
    "children": ["header-row#mobile-2"]
  },
  "header-row#mobile-1": {
    "children": ["telemarketing"],
    "props": {
      "fullWidth": true,
      "sticky": true
    }
  },
  "header-row#mobile-2": {
    "children": [
      "logo#header",
      "header-spacer",
      "login#header-mobile",
      "minicart"
    ],
    "props": {
      "sticky": true
    }
  },
  "login#header-mobile": {
    "props": {
      "showIconProfile": true
    }
  },
  "logo#header": {
    "props": {
      "width": 132,
      "height": 32,
      "mobileWidth": 90,
      "mobileHeight": 32
    }
  }
}
